package com.group8.ruins_of_light;

public class Record {
	
	private String nombre;
	private int puntuacion;

	public Record() {
	}

	public Record(String nombre, int puntuacion) {
		super();
		this.nombre = nombre;
		this.puntuacion = puntuacion;
	}
	
	public String GetName() {
		return nombre;
	}
	
	public int GetScore() {
		return puntuacion;
	}
	
	public void SetId(int id) {
		
	}
	
	@Override
	public String toString() {
		return nombre + String.valueOf(puntuacion);
	}
}
